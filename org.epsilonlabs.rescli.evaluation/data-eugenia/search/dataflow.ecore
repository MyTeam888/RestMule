<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="dataflow" nsURI="http://de.abilov.meta.dataflow" nsPrefix="dataflow">
  <eClassifiers xsi:type="ecore:EClass" name="DFModel" eSuperTypes="#//DFNamespace">
    <eAnnotations source="gmf.diagram">
      <details key="onefile" value="true"/>
      <details key="units" value="Pixels"/>
      <details key="diagram.extension" value="dflow"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="targetNamespace" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFNamespace" eSuperTypes="#//DFModelElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ownedElement" upperBound="-1"
        eType="#//DFModelElement" containment="true" eOpposite="#//DFModelElement/namespace">
      <eAnnotations source="gmf.compartment">
        <details key="collapsible" value="true"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFModelElement" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="namespace" eType="#//DFNamespace"
        eOpposite="#//DFNamespace/ownedElement"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="constraint" upperBound="-1"
        eType="#//DFConstraint" eOpposite="#//DFConstraint/constrainedElement"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFClassifier" abstract="true" eSuperTypes="#//DFNamespace">
    <eAnnotations source="gmf.node">
      <details key="label.icon" value="false"/>
      <details key="border.width" value="2"/>
      <details key="border.color" value="0,0,0"/>
      <details key="label" value="name"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFDataElement" eSuperTypes="#//DFModelElement">
    <eAnnotations source="gmf.node">
      <details key="label.icon" value="false"/>
      <details key="border.width" value="2"/>
      <details key="border.color" value="0,0,0"/>
      <details key="label" value="name"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="includedData" upperBound="-1"
        eType="#//DFDataElement" containment="true" eOpposite="#//DFDataElement/refinedData">
      <eAnnotations source="gmf.compartment">
        <details key="collapsible" value="true"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refinedData" eType="#//DFDataElement"
        eOpposite="#//DFDataElement/includedData"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="store" upperBound="-1"
        eType="#//DFDataStore" eOpposite="#//DFDataStore/contents">
      <eAnnotations source="gmf.link">
        <details key="color" value="0,0,0"/>
        <details key="width" value="2"/>
        <details key="style" value="dot"/>
      </eAnnotations>
    </eStructuralFeatures>
    <eStructuralFeatures xsi:type="ecore:EReference" name="flow" upperBound="-1" eType="#//DFDataFlow"
        eOpposite="#//DFDataFlow/dataType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dataExpression" eType="ecore:EClass ../../de.abilov.dsl.dataexpr/model/generated/Dataexpr.ecore#//DEExpr"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFConstraint" eSuperTypes="#//DFModelElement">
    <eAnnotations source="gmf.node">
      <details key="label.icon" value="false"/>
      <details key="border.width" value="2"/>
      <details key="border.color" value="0,0,0"/>
      <details key="label" value="body"/>
      <details key="figure" value="rounded"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="body" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="constrainedElement" upperBound="-1"
        eType="#//DFModelElement" eOpposite="#//DFModelElement/constraint">
      <eAnnotations source="gmf.link">
        <details key="color" value="0,0,0"/>
        <details key="width" value="2"/>
        <details key="target.decoration" value="arrow"/>
      </eAnnotations>
    </eStructuralFeatures>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFExternalEntity" eSuperTypes="#//DFClassifier">
    <eAnnotations source="gmf.node">
      <details key="figure" value="rectangle"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ref" eType="ecore:EClass ../../br.ufpe.eercase/model/EERCASE.ecore#//Entity"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFDataFlow" eSuperTypes="#//DFModelElement">
    <eAnnotations source="gmf.link">
      <details key="source" value="source"/>
      <details key="target" value="target"/>
      <details key="incoming" value="true"/>
      <details key="color" value="0,0,0"/>
      <details key="width" value="2"/>
      <details key="source.constraint" value="self &lt;> oppositeEnd"/>
      <details key="label" value="name"/>
      <details key="target.decoration" value="filledclosedarrow"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="childFlow" eType="#//DFDataFlow"
        eOpposite="#//DFDataFlow/refinedFlow"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="refinedFlow" eType="#//DFDataFlow"
        eOpposite="#//DFDataFlow/childFlow"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="flowType" eType="#//DFFlowKind"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="dataType" lowerBound="1"
        upperBound="-1" eType="#//DFDataElement" eOpposite="#//DFDataElement/flow"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" eType="#//DFClassifier"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" eType="#//DFClassifier"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFInternalEntity" abstract="true" eSuperTypes="#//DFClassifier"/>
  <eClassifiers xsi:type="ecore:EClass" name="DFDataTransformation" eSuperTypes="#//DFInternalEntity">
    <eAnnotations source="gmf.node">
      <details key="figure" value="ellipse"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="multiple" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="implementation" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="includedElements" upperBound="-1"
        eType="#//DFInternalEntity"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFFlowModifier" abstract="true" interface="true"
      eSuperTypes="#//DFInternalEntity">
    <eAnnotations source="gmf.node">
      <details key="figure" value="ellipse"/>
      <details key="label.placement" value="external"/>
      <details key="label.readOnly" value="true"/>
      <details key="size" value="5,5"/>
      <details key="resizable" value="false"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFSplit" abstract="true" interface="true"
      eSuperTypes="#//DFFlowModifier"/>
  <eClassifiers xsi:type="ecore:EClass" name="DFJoin" abstract="true" interface="true"
      eSuperTypes="#//DFFlowModifier"/>
  <eClassifiers xsi:type="ecore:EClass" name="DFSubsetSplit" eSuperTypes="#//DFSplit">
    <eAnnotations source="gmf.node">
      <details key="label.text" value="Subset split"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFCloneSplit" eSuperTypes="#//DFSplit">
    <eAnnotations source="gmf.node">
      <details key="label.text" value="Clone split"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFAndJoin" eSuperTypes="#//DFJoin">
    <eAnnotations source="gmf.node">
      <details key="label.text" value="And join"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFOrJoin" eSuperTypes="#//DFJoin">
    <eAnnotations source="gmf.node">
      <details key="label.text" value="Or join"/>
    </eAnnotations>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DFDataStore" eSuperTypes="#//DFInternalEntity">
    <eAnnotations source="gmf.node">
      <details key="figure" value="rectangle"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="contents" upperBound="-1"
        eType="#//DFDataElement" eOpposite="#//DFDataElement/store"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="DFFlowKind">
    <eLiterals name="Discrete" value="1"/>
    <eLiterals name="Continuous" value="2"/>
  </eClassifiers>
</ecore:EPackage>
